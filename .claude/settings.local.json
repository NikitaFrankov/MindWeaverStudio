{
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(./gradlew:*)",
      "Bash(git add:*)",
      "Bash(git push:*)",
      "Bash(git commit:*)",
      "Bash(tree:*)",
      "Bash(kotlinc:*)"
    ],
    "deny": [],
    "defaultMode": "acceptEdits"
  },
  "statusLine": {
    "type": "command",
    "command": "/opt/homebrew/bin/ccusage statusline",
    "padding": 0
  }
}